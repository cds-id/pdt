{
  "name": "vite-tailwind-boilerplate",
  "description": "Boilerplate Projects",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "Indra Gunanda",
    "email": "indra.gunanda@mail.com",
    "url": "https://github.com/cds-id"
  },
  "bugs": {
    "url": "https://github.com/cds-id/vite-tailwind-boilerplate/issues",
    "email": "indra.gunanda@mail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --max-warnings=0 --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky",
    "commit": "git add . && cz",
    "push": "git push --follow-tags origin HEAD",
    "release": "release-it",
    "setup": "node template-setup.js",
    "setup:project": "node scripts/setup-project.js",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}",
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reduxjs/toolkit": "^2.5.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.574.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "recharts": "^3.7.0",
    "redux-persist": "^6.0.0",
    "shadcn-ui": "^0.9.5",
    "tailwind-merge": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.17.11",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "3.1.1",
    "prompts": "^2.4.2",
    "release-it": "^17.11.0",
    "shadcn": "^3.8.5",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.19.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
